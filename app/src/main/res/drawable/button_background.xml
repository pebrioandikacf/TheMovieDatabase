<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#FF5722" /> <!-- Warna ketika tombol ditekan -->
            <corners android:radius="20dp" /> <!-- Sudut melingkar -->
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFC107" /> <!-- Warna normal tombol -->
            <corners android:radius="20dp" /> <!-- Sudut melingkar -->
        </shape>
    </item>
</selector>
